{
  "presets": [
    "node8"
  ],
  "plugins": [
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "web": "./src/web/",
          "socket": "./src/socket/",
          "services": "./src/services/",
          "repositories": "./src/repositories/",
          "providers": "./src/providers/",
          "utilities": "./src/utilities/",
          "actions": "./src/actions/"
        }
      }
    ],
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}
